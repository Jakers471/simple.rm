{
  "name": "swarm-strategy-tests",
  "version": "1.0.0",
  "description": "Comprehensive test suite for swarm strategies",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=integration",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=strategies",
    "test:performance": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=performance",
    "benchmark": "node performance/benchmark-runner.js",
    "coverage:report": "node coverage-report.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "claude-flow": "^2.7.0"
  }
}
